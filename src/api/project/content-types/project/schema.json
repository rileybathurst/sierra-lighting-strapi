{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "areas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::area.area",
      "mappedBy": "project"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[a-z0-9]*$"
    },
    "teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team.team",
      "inversedBy": "projects"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "lights": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::light.light",
      "mappedBy": "projects"
    },
    "excerpt": {
      "type": "text"
    },
    "ogimage": {
      "type": "string"
    },
    "venue": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::venue.venue",
      "inversedBy": "projects"
    },
    "vendors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::vendor.vendor",
      "inversedBy": "projects"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "hero": {
      "type": "boolean"
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "mappedBy": "projects"
    },
    "darkImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
