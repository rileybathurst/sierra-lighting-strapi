{
  "kind": "collectionType",
  "collectionName": "areas",
  "info": {
    "singularName": "area",
    "pluralName": "areas",
    "displayName": "Area",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "tagline": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "california",
        "nevada"
      ]
    },
    "region": {
      "type": "enumeration",
      "enum": [
        "northlake",
        "truckee",
        "reno",
        "sparks"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "required": true,
      "regex": "^[a-z0-9]*$",
      "unique": true
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "areas"
    },
    "venues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::venue.venue",
      "mappedBy": "area"
    },
    "excerpt": {
      "type": "text"
    }
  }
}
